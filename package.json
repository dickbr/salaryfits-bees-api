{
  "name": "maxmilhas-cpf-api",
  "version": "0.0.1",
  "description": "API Consulta CPF",
  "author": "Jean Pierre",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "node dist/index.js",
    "dev": "tsnd --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/index.ts",
    "test": "jest",
    "test:uncov": "jest --coverage=false",
    "test:e2e": "exit 1",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/database/postgres/datasource.ts",
    "migration:gen": "read name && yarn typeorm migration:generate ./src/database/postgres/migrations/$name",
    "migration:create": "read name && npx typeorm-ts-node-commonjs migration:create ./src/database/postgres/migrations/$name",
    "migration:run:dev": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/swagger": "^8.6.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/core": "^10.0.3",
    "@nestjs/platform-fastify": "^10.0.3",
    "@nestjs/typeorm": "^10.0.1",
    "aws-sdk": "^2.1560.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.4.4",
    "fastify": "^4.21.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
